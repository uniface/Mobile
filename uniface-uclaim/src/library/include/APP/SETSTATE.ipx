<DAT name="ULABEL">SETSTATE</DAT>
<DAT name="ULAN">P</DAT>
<DAT name="UTEXT" xml:space='preserve'>variables
	String vLayoutFile, vLayout
endvariables

; ----------------------------------------------
; Generate response
; ----------------------------------------------
selectcase $webresponsetype
; Update page
case "UPDATE"
	; Generate data
	websave
	if ($procerror &lt; 0)
		webmessage/error "websave failed %%$instancename%%% -  (Set State)%%^$procerror = %%$procerror%%%"
		return 0
	endif
; Initial page
case "FULLPAGE"
	; Generate layout
	#if (&lt;LAYOUT_SOURCE&gt;=FILE)
		;Get the layout from a file
		#ifundefined EXTERNAL_HTML
			#error LAYOUT_SOURCE of "FILE" specified but no EXTERNAL_HTML define found
		#endif
;---------------------------------------------------------------------------------------------
;		weblayout $concat(".\hts\", $lowercase("&lt;EXTERNAL_HTML&gt;"),".html") 
;---------------------------------------------------------------------------------------------
;----Replace standard weblayout function with following routine that performs 4GL substitution
;----of $text() and sets new proc option $instancelayout
;---------------------------------------------------------------------------------------------
		if(!$logical("DEPLOYED_LAYOUT")) ; Local version so parse layout at runtime
			fileload  $concat(".\hts\", $lowercase("&lt;EXTERNAL_HTML&gt;"),".html"), vLayout
			$instancelayout=lpsubst(vLayout, $variation, $language)
			weblayout	
		else 			; deployed so parsing of layout files already done
			weblayout $concat(".\hts\%%$language%%%\", $lowercase("&lt;EXTERNAL_HTML&gt;"),".html")
		endif	
;--------------------------------------------------------------------------------------------------------
	#elseif (&lt;LAYOUT_SOURCE&gt;=DYNAMIC)
		;Get a file to use from GET_LAYOUT_PROC
		#ifundefined GET_LAYOUT_PROC
			#error LAYOUT_SOURCE of "DYNAMIC" specified but no GET_LAYOUT_PROC define found
		#endif
		vLayoutFile = $lowercase( &lt;GET_LAYOUT_PROC&gt;() )
		if(vLayoutFile != "")
;--------------------------------------------------------------------------------------------------------
;			weblayout $concat(".\hts\", vLayoutFile,".html")   ;Set initial weblayout with empty entry
;---------------------------------------------------------------------------------------------
;----Replace standard weblayout function with following routine that performs 4GL substitution
;----of $text() and sets new proc option $instancelayout
;---------------------------------------------------------------------------------------------
		if(!$logical("DEPLOYED_LAYOUT")) ; Local version so parse layout at runtime
			fileload $concat(".\hts\", vLayoutFile,".html"), vLayout
			$instancelayout=lpsubst(vLayout, $variation, $language)
			weblayout
		else 			; deployed so parsing of layout files already done
			weblayout $concat(".\hts\%%$language%%%\", vLayoutFile,".html")
		endif
;--------------------------------------------------------------------------------------------------------
		else
			;If &lt;GET_LAYOUT_PROC&gt; didn't return anything then just use the internal layout
			weblayout
		endif
;--------------------------------------------------------------------------------------------------------
	#elseif (&lt;LAYOUT_SOURCE&gt;=INSTANCE)
		;Use the DSP's instance name to find the layout
;--------------------------------------------------------------------------------------------------------
;		weblayout $concat(".\hts\", $lowercase($instancename), ".html")   ;Set initial weblayout with empty entry
;---------------------------------------------------------------------------------------------
;----Replace standard weblayout function with following routine that performs 4GL substitution
;----of $text() and sets new proc option $instancelayout
;---------------------------------------------------------------------------------------------
		if(!$logical("DEPLOYED_LAYOUT")) ; Local version so parse layout at runtime
			fileload $concat(".\hts\", $lowercase($instancename), ".html"), vLayout
			$instancelayout=lpsubst(vLayout, $variation, $language)
			weblayout
		else 			; deployed so parsing of layout files already done
			weblayout $concat(".\hts\%%$language%%%\", $lowercase($instancename),".html")
		endif
;--------------------------------------------------------------------------------------------------------
	#else
		;Otherwise use the internal layout
		weblayout
	#endif
	if ($procerror &lt; 0)
		webmessage/error "weblayout failed %%$instancename%%%- (Set State)%%^$procerror = %%$procerror%%%"
		return $status
	endif
	; Generate component definitions
	webdefinitions
	if ($procerror &lt; 0)
		webmessage/error "webdefinitions failed %%$instancename%%%- (Set State)%%^$procerror = %%$procerror%%%"
		return $status
	endif

	#ifdefined PAGE_CSS
		#for STYLESHEET = (&lt;PAGE_CSS&gt;)
			putitem $webinfo("CSS"), -1, &lt;STYLESHEET&gt;
		#endfor
	#endif

	#ifdefined PAGE_JS
		#for JS = (&lt;PAGE_JS&gt;)
			putitem $webinfo("JAVASCRIPT"), -1, &lt;JS&gt;
		#endfor
	#endif

	; Generate data
	websave
	if ($procerror &lt; 0)
		webmessage/error "websave failed %%$instancename%%%- (Set State)%%^$procerror = %%$procerror%%%"
		return 0
  endif

  #ifdefined POST_FULLPAGE_SET_STATE
  ;This DSP defines a post fullpage set state operation, so call it
  $instancehandle-&gt;postFullPageSetState()
  #endif


endselectcase

return 0</DAT>
