;----------------------------------------------------------------
; Reset the page. We don't need anything from the server so
; this can be done with a weboperation. We might even be logged
; out at this point so going back to the server could trigger
; a not authorised error.
;----------------------------------------------------------------
weboperation detach
;----------------------------------------------------------------

javascript

	this.getEntity("DUMMY.DUMMY").clear();

endjavascript

end ;detach


;----------------------------------------------------------------
; Display a screen.
;	pPage : IN - The alias for the page we want to show
;----------------------------------------------------------------
operation showScreen
;----------------------------------------------------------------
public web

params
	String pPage : IN
endparams

variables
	Struct  vMenuMapStruct
	Numeric vSize
	Numeric i
	String  vAlias
	String  vDspName
endvariables

	call lpLoadMenuConfig(vMenuMapStruct)
	if($status &lt; 0)
		return -1
	endif	

	;Find the DSP name based on the page alias
	i = 1
	vSize = vMenuMapStruct-&gt;*-&gt;$collsize
	while (i &lt;= vSize)

		vAlias = vMenuMapStruct-&gt;*{i}-&gt;"alias"

		if(vAlias = pPage)
			vDspName = vMenuMapStruct-&gt;*{i}-&gt;"dsp"
			break
		endif

		i = i + 1
	endwhile

	$instancehandle-&gt;loadScreen(vDspName)
	return 0

end ;showScreen


