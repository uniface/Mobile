;----------------------------------------------------------------
operation loadScreen
;----------------------------------------------------------------
public web

params
	String pScreen  : IN
	String pOptions : IN
endparams

variables
	Struct  vOptionsStruct
	Numeric vCurrentOcc
	String  vClassName
endvariables

	jsonToStruct vOptionsStruct, pOptions
