<DAT name="SFUNC" xml:space='preserve'>#define CREATE_JS
;----------------------------------------------------------------
;
;----------------------------------------------------------------
weboperation createJs
;----------------------------------------------------------------
params
	String pOptionsJson : IN
endparams

javascript
var thisInstance = this;
mapp.menuInstance = this;

$(function() {
    $("button[id^='ufld:BACK_BUTTON']").on('singletap', function() {
        mapp.goBack();
    });
});


try {
	var options = JSON.parse(pOptionsJson);
} catch(e) {
	console.error("&lt;$componentname&gt;: Parse error");
}

/*
 *  Clobber this function so we can manage history in Uniface
 */
$.UISetHashOnUrl = function(url, delimiter) {};


$(function() {

	$.UISlideout({
		dynamic: true,
		callback: function(option) {
			console.log(option);

			var article = $(option).attr('data-show-article');

			if(article == "logout") {
				//The logout option has been pressed
				uniface.getInstance().activate("logOff")
				return;	
			}

			$('.slide-out').removeClass('open');
			mapp.showScreen(article);
		}
	});
	// Populate the slide out:
/*	$.UISlideout.populate([
		{home:'My Insurance'},
		{claim:'Make a Claim'},
		{claims:'My Claims'},
		{messages:'Messages'},
		{contacts:'Contacts'},
		{logout:'Logout'}
	]); */

	$.UISlideout.populate(eval(options.menuitems));

	mapp.showScreen(options.path);

});


endjavascript

end ;createJs

