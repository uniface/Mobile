<DAT name="INIT" xml:space='preserve'>public web

variables
	String  vAppConfigJson
	Boolean vMockCordovaApi
	String  vSessionAttributes
	String  vUserName
	String  vPath
endvariables

;Get anything passed in on the query string
vMockCordovaApi = $item("MOCKCORDOVAAPI", $webinfo("INPUT"))

;Check the session attributes for a user name. A user name logged against the
;session indicates that the user is logged in.
vSessionAttributes = $item("SESSIONATTRIBUTES", $webinfo("webservercontext"))
vUserName          = $item("UserName", vSessionAttributes)


;Get the first page in the menu configuration
vPath = $appConfig$-&gt;"menu"-&gt;"children"-&gt;*{1}-&gt;$name

;Do any client side setup
structToJson vAppConfigJson, $appConfig$
webactivate $instancename.setup(vPath, vAppConfigJson, vMockCordovaApi)

if(vUserName == "")
	;If the user isn't logged in then show the login page
	$instancehandle-&gt;showLogin(vPath)
else
	;If the user is logged in then show the main application
	$instancehandle-&gt;showApplication(vPath)
endif

return 0</DAT>
