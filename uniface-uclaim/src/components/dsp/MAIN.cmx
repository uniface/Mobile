      commit
   endif
endif
</DAT>
<DAT name="GENERAL" xml:space='preserve'>;---------------------------------------------------------------
entry lpGetLayout
	;---------------------------------------------------------------
	
	return $logical("MAIN_HTML")
	
end ;lpGetLayout

entry lpsetlanguage
	variables
		string vLang, vopt
	endvariables
	
	if($logical("BROWSER_NLS"))
		$variation = "APP"
		vLang = $item("accept-language", $webinfo("httpRequestHeaders"))
		vLang = $replace(vLang,1,",",";",-1)
		vLang = $replace(vLang,1,";","&uSEP;",-1)
		getitem vOpt,vLang,1
		selectcase $lowercase(vOpt)
			case 'en*'
				$language = "USA"
			case 'ja*'
				$language = "JA"
			case 'de*'
				$language = "DE"
			case 'fr*'
				$language = "FR"
			case 'es*'
				$language = "ES"
			case 'pt*'
				$language = "PT"
			case 'it*'
				$language = "IT"
			case 'nl*'
				$language = "NL"
			case 'zh*'
				$language = "ZH"
			case 'fi*'
				$language = "FI"
			elsecase
				$language = "USA"
		endselectcase
	endif
end

#include App:lpLoadMenuConfig
#include App:dspLocalProcs</DAT>
