<DAT name="SFUNC" xml:space='preserve'>;--r-------------------------------------------------------
;
;---------------------------------------------------------
operation init
;---------------------------------------------------------

	;If configured set language to browser setting
	call lpsetlanguage()
	;Load the menu config
	call lpLoadMenuConfig($appConfig$)
	if($status &lt; 0)
		return -1
	endif

end ;init


;---------------------------------------------------------
;
;---------------------------------------------------------
operation postFullPageSetState
;---------------------------------------------------------
variables
	Numeric i
endvariables

	;Add JS - adding it here ensures it's added after the Uniface JS
	i = 1

	while (i &lt;= $appConfig$-&gt;"js"-&gt;*-&gt;$collSize)
		;putmess "  vStruct1-&gt;a{%%i%%%} has value %%(vStruct1-&gt;a{i})%%%"
		putitem $webinfo("JAVASCRIPT"), -1, $appConfig$-&gt;"js"-&gt;*{i}
		i = i + 1
	endwhile

	return 0

end ;postFullpageSetState


;---------------------------------------------------------
;
;---------------------------------------------------------
weboperation setup
;---------------------------------------------------------
params
	String  pPage    : IN
	String  pMenuMap : IN
	Boolean pMockCordovaApi : IN
endparams

javascript

	try {
		var appConfig = JSON.parse(pMenuMap);
		mapp.config.menuMap = appConfig.menu.children;
	} catch(e) {
		console.error("&lt;$componentname&gt;: Parse error. " + e.message);
	}

	//Replace the current history entry with our own one
	var state = {page: pPage};
	history.replaceState(state, null);

	/*
	 *  Register a listener against the popState event
	 *  to handle users pressing the back and forward
	 *  buttons.
	 */
	window.addEventListener("popstate", function(e) {

		if(!e.state) {
			return
		}

		var path = e.state.path;
		if(path != "") {

			//What DSP are we moving to based on the page we just popped off the history stack?
			var movingToInstance = mapp.config.getInstanceNameByPath(path);

			if(mapp.refreshOnNextBack) {
				//We're moving back to the last page and it needs refreshing
				mapp.refreshOnNextBack = false;
				mapp._backToPage(movingToInstance, true);
			}
			else {
				//We're just moving back to the last page
				mapp._backToPage(movingToInstance);
			}

		}

		mapp.currentPath = path;
		console.log("New current path: " + path);

	});




endjavascript

end ;setup


;---------------------------------------------------------
operation showApplication
;---------------------------------------------------------
partner web

params
	String pPage : IN
endparams

variables
	String vMenuDsp
	String vPage
	Struct vParams
	String vMenuItems
	String vSessionAttributes
endvariables

	vMenuDsp = $appConfig$-&gt;"menu"-&gt;"dsp"

	if(pPage != "")
		vPage = pPage
	else
		vPage = REQUESTED_PAGE.DUMMY
	endif

	if(vPage == "")
		vPage = $appConfig$-&gt;"menu"-&gt;"children"-&gt;*{1}-&gt;$name
	endif

	vMenuItems = $appconfig$-&gt;menu-&gt;params-&gt;menuitems

	vParams-&gt;"menuitems" = vMenuItems

	vParams-&gt;"path" = vPage
	CONTENT.DUMMY = lpGetDsp(vMenuDsp, vParams, "")

	return 0

end ;showApplication


;---------------------------------------------------------
operation showLogin
;---------------------------------------------------------
partner web

params
	String pPage : IN
endparams

variables
	String vLoginDsp
	String vInstanceName
endvariables

	REQUESTED_PAGE.DUMMY = pPage

	vLoginDsp     = $appConfig$-&gt;"login"-&gt;"dsp"
	vInstanceName = $appConfig$-&gt;"login"-&gt;"instanceName"

	newinstance vLoginDsp, vInstanceName
	activate vInstanceName.appCreate("")
	CONTENT.DUMMY = vInstanceName

	return 0

end ;showLogin


;---------------------------------------------------------
operation logOff
;---------------------------------------------------------
public web

	; Close the current session
	putitem/id $webinfo("SESSIONCOMMANDS"), "ChangeSession", "CLOSE"
	webactivate $instancename.fullPageRefresh()

	return 0

end ;logOff


;---------------------------------------------------------
weboperation fullPageRefresh
;---------------------------------------------------------
javascript

	location.reload();

endjavascript
end ;fullPageRefresh



