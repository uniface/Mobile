;----------------------------------------------------------------
operation init
;----------------------------------------------------------------

	newinstance "APP_SIGNIN_SVC", $signinService$
	if($status &lt; 0)
		return $status
	endif

	$remoteAddr$ = $item( "REMOTE_ADDR", $item("SERVERVARIABLES", $webinfo("WEBSERVERCONTEXT")) )

end ;init


#define POST_SETUP
;----------------------------------------------------------------
; 
;----------------------------------------------------------------
operation postSetup
;----------------------------------------------------------------
variables
	Numeric vAttempts
endvariables

	call lpGetLoginAttempts(vAttempts)
	if($status &lt; 0)
		activate "&lt;MAIN_DSP&gt;".showMessage($text(M_RUNERROR), $status)
		return 0
	endif

	call lpShowCaptcha(vAttempts)
	if($status &lt; 0)
		return $status
	endif

	return 0

end ;postSetup



;----------------------------------------------------------------
weboperation detach
;----------------------------------------------------------------

javascript
	this.getEntity("DUMMY.DUMMY").clear();
	document.getElementById("username").value = "";
	document.getElementById("password").value = "";
endjavascript

end ;detach


;----------------------------------------------------------------
operation doLogin
;----------------------------------------------------------------
public web

scope
	input
	output
;	operation $instancename.showMessage
endscope

params
	String pUserName     : IN
	String pPassword     : IN
	String pTitle			: IN
	String pFirstname		: IN
	String pLastname		: IN
	String pAddress			: IN
	String pCity			: IN
	String pState			: IN
	String pPostalCode		: IN
	String pSubmitParams : IN
endparams

variables
	Struct  vSubmitParams
	Handle  vSigninService
	String  vServerVariables
	Numeric vAttempts
	String  vPluginParams
	Boolean vAuthorised
	String  vMessage
	String  vUserId
endvariables

	if(pSubmitParams != "")
		jsonToStruct vSubmitParams, pSubmitParams
	endif

;	call lpGetLoginAttempts(vAttempts)
;	if($status &lt; 0)
;		activate $instancename.showMessage($text(M_RUNERROR), $status)
;		return 0
;	endif

	putitem/id vPluginParams, "CHALLENGE",   vSubmitParams-&gt;challenge
	putitem/id vPluginParams, "RESPONSE",    vSubmitParams-&gt;response
	putitem/id vPluginParams, "REMOTE_ADDR", $remoteAddr$

	;Check the provided credentials
	call lpDoLogin(pUserName, pPassword, pTitle, pFirstname, pLastname, pAddress, pCity, pState, pPostalCode, vPluginParams, vAttempts, vAuthorised, vUserId, vMessage)
	if($status &lt; 0)
		activate $instancename.showMessage($text(M_RUNERROR), $status)
		return 0
	endif

	;The check failed, re-present the login page to the user
	if(!vAuthorised)
		activate $instancename.showMessage($text(M_LOGINERR), vMessage)
		if(CAPTCHA.DUMMY != "")
			call lpReloadCaptcha()
		else
			call lpShowCaptcha(vAttempts)
			if($status &lt; 0)
				activate $instancename.showMessage($text(M_RUNERROR), $status)
				return 0
			endif
		endif

		return 0
	endif

	;The credentials are valid, create a new "logged in" session
;	call lpCreateAuthenticatedSession(pUserName, vUserId)
;	if($status &lt; 0)
;		activate $instancename.showMessage($text(M_AUTHSESSERR), $status)
;		return 0		
;	endif

	;Remove the login DSP and show the main application page
	activate "login".showApplication("")
	if($status &lt; 0)
		activate $instancename.showMessage($text(M_RUNERROR), $status)
		return 0		
	endif

	CAPTCHA.DUMMY = ""
	return 0

end ;doLogin
;---------------------------------------------------------
operation showMessage
;---------------------------------------------------------
public web

params
	String pMessage     : IN
	String pDescription : IN
endparams

	MESSAGE.DUMMY     = pMessage
	DESCRIPTION.DUMMY = pDescription

	$fieldproperties("MESSAGE_AREA.DUMMY", "style:display") = "style:display=block"

	return 0

end ;showMessage


;---------------------------------------------------------
operation clearMessage
;---------------------------------------------------------
public web

	MESSAGE.DUMMY     = ""
	DESCRIPTION.DUMMY = ""

	$fieldproperties("MESSAGE_AREA.DUMMY", "style:display") = "style:display=none"

	return 0

end ;clearMessage


