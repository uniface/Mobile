<DAT name="GENERAL" xml:space='preserve'>;-----------------------------------------------------------------------
;
;-----------------------------------------------------------------------
entry lpCreateAuthenticatedSession
;-----------------------------------------------------------------------
params
	String pUserName : IN
	String pUserId   : IN
endparams

	selectcase $logical("STATE_MODULE")
		case "TOMCAT"
			;Send command to WRD to create a new session
			putitem/id $webinfo("SESSIONCOMMANDS"), "ChangeSession", "NEW"
			putitem/id $webinfo("SESSIONCOMMANDS"), "SetAttributes", "UserName=%%pUserName%%%&uSEP;UserId=%%pUserId%%%"

			;Optionally, if you want to keep the session context in the new session,
			;ask WRD to call back to your "copySessionContext" operation.
			;putitem/id $webinfo("SESSIONCOMMANDS"), "WrdActivate", "%%$instancename.copySessionContext"

		case "SESSION_URL"
			;newinstance "SESSION_SVC", vSessionHandle
			;$status = vSessionHandle-&gt;

		elsecase
			return -1

	endselectcase

	return 0

end ;lpCreateAuthenticatedSession


;-----------------------------------------------------------------------
;
;-----------------------------------------------------------------------
entry lpGetLoginAttempts
;-----------------------------------------------------------------------
params
	Numeric pAttempts : OUT
endparams

	$status = $signinService$-&gt;getAttempts($remoteAddr$, pAttempts)
	if($status &lt; 0)
		return $status
	endif

	return 0

end ;lpGetLoginAttempts


;-----------------------------------------------------------------------
;
;-----------------------------------------------------------------------
entry lpDoLogin
;-----------------------------------------------------------------------
params
	String  pUserName     : INOUT
	String  pPassword     : IN
	String  pTitle		   : IN
	String  pFirstname    : IN
	String  pLastName     : IN
	String  pAddress      : IN
	String  pCity         : IN
	String  pState        : IN
	String  pPostalCode   : IN
	String  pPluginParams : IN
	Numeric pAttempts     : INOUT
	Boolean pAuthorised   : OUT
	String  pUserId       : OUT
	String  pMessage      : OUT
endparams


;	if(pAttempts &gt; 2)
;		call lpCheckCaptcha(pPluginParams, pAuthorised, pMessage)
;		if($status &lt; 0)
;			return $status
;		endif

;		if(!pAuthorised)
;			return 0
;		endif
;	endif

	$status = $signinService$-&gt;checkCredentials(pUserName, pPassword, $remoteAddr$, pUserId, pAttempts, pAuthorised)
	if($status &lt; 0)
		return $status
	endif

	if(!pAuthorised)
		pMessage = $text(M_LOGINERR)
		return 0
	endif

	return 0

end ;lpDoLogin


;-----------------------------------------------------------------------
;
;-----------------------------------------------------------------------
entry lpShowCaptcha
;-----------------------------------------------------------------------
params
	Numeric pAttempts : IN
endparams

	if(pAttempts &gt; 2)
		CAPTCHA.DUMMY = lpGetDsp("APP_RECAPTCHA", "")
	endif

	return 0

end ;lpShowRecaptcha


;-----------------------------------------------------------------------
;
;-----------------------------------------------------------------------
entry lpCheckCaptcha
;-----------------------------------------------------------------------
params
	String  pCaptchaParams : IN
	Boolean pAuthorised    : OUT
	String  pMessage       : OUT
endparams

variables
	Boolean vResult
	String  vErrorCode
endvariables

	activate "%%CAPTCHA.DUMMY%%%".verify(pCaptchaParams, vResult, vErrorCode)
	if($status &lt; 0)
		return $status
	endif

	if(!vResult)
		pAuthorised = 0
		pMessage    = $text(M_RECAPTCHAERR)
		return 0
	endif

	pAuthorised = 1
	return 0

end ;lpCheckCaptcha


;-----------------------------------------------------------------------
;
;-----------------------------------------------------------------------
entry lpReloadCaptcha
;-----------------------------------------------------------------------

	activate "%%CAPTCHA.DUMMY%%%".reloadRecaptcha()
	return 0

end ;lpReloadCaptcha


;---------------------------------------------
entry lpGetLayout
;---------------------------------------------
returns String

variables
	String vUIType
endvariables

	vUIType = $lowercase( $logical("UI_TYPE") )

	selectcase vUIType
		case "chui"
			return "chui_login"
		case "desktop"
			return "dtop_login"
		case "polymer"
			return "mpoly_login"
		elsecase
			return "dtop_login"
	endselectcase

end ;lpGetLayout


